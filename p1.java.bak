class p1
{
	public static void main(String args[])
	{
		int i=10;
		// Unary operators
		// ++ increment   -- decrement
		i++;
		System.out.println(i);
		
		int j=10;
		j--;
		System.out.println(j);
		
		int k=5;
		++k;
		System.out.println(k);
		
		int l=8;
		l++;
		System .out.println(l);
		
		int m=4;
		int n=2;
		m+=n;
		System.out.println(m);
	}
}